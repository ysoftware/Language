func printf(format: String, arguments: Int32, ...) #foreign;

X :: "Position.x is %d\n";
Y :: "Position.y is %d\n";

struct Point {
    x: Int;
    y: Int;
}

func main(count: Int, arguments: Int8**) -> Int {

    position : Point;
    
    position.x = 15;
    position.y = 30;

    printf(X, position.x);
    printf(Y, position.y);

    return 0;
}

