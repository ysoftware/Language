func printf(format: String, arguments: Int, ...) #foreign; 

hello :: "a is %d";

newline :: """


"""
;

func printn() -> Int {
    printf(newline, 0); 
    return 1; 
}

func main() -> Int {
    b :: 500;
    a := 0; 

    while (a < b) {
        a = a + 1;
        printf(hello, a);
        printn();
    }
    return 0;
}