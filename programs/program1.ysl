// A simple program that adds 2 numbers and prints them to the console

// don't forget to add some sort of import io

func main() -> Int {

    a : Int : 3                     // let a: Int = 3
    b : Int = 1                     // var b: Int = 1
    b = a + b                       // b = a + b

    print("%d\n", b)                // print("\(b)\n")
}