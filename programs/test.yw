struct Node<Value> {
    next: Node<Value>*;
    value: Value;
}

struct Pair<Value, Value2> {
    left: Value*;
    right: Value2*;
}

func list_append(head: Node<Int>*, value: Int) -> Node<Int>* {
    node := new Node<Int>;
    node.value = value;
    current := head;
    while (current.next != null) {
        current = current.next;
    }
    current.next = node;
    return node
}

